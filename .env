#Spring Boot app name and version
APP_NAME=Shop_Inventory_POS-1.0.jar

#Spring Boot app properties
DB_HOST=jdbc:postgresql://postgres
DB_PORT=5432
DB_DRIVER=org.postgresql.Driver
HIBERNATE_DIALECT=org.hibernate.dialect.PostgreSQL81Dialect

#Postgres settings for owner(superuser)
