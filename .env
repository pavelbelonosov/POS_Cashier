POSTGRES_USER=shoppos
POSTGRES_PASSWORD=166831
POSTGRES_SCHEMA=shoppos

##Postgres settings for app_client(with CRUD tables privileges only);
#If any changes see ./src/main/resources/db/migration/V1__initialize_tables_sequence.sql & V0.1__init_roles.sql
CLIENT_DB_USER=client
CLIENT_DB_USER_PASSWORD=client166831